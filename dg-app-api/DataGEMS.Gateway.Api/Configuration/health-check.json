{
  "HealthCheck": {
    "Folder": {
      "Paths": [ "%{LogsPath}%" ]
    },
    "Memory": {
      "maxPrivateMemoryBytes": 10485760, //10MB = 10485760B
      "maxProcessMemoryBytes": 10485760, //10MB = 10485760B
      "maxVirtualMemoryBytes": 10485760 //10MB = 10485760B
    },
    "Endpoint": {
      "IsEnabled": true,
      "EndpointPath": "/health-check",
      "RequireHosts": null,
      "IncludeTag": null,
      "AllowCaching": false,
      "HealthyStatusCode": 200,
      "DegradedStatusCode": 200,
      "UnhealthyStatusCode": 503,
      "VerboseResponse": true
    }
  }
}