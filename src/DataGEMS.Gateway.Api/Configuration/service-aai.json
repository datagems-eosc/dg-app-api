{
	"AAIService": {
		"Service": {
			"Scope": "dg-app-api-keycloak-admin",
			"BaseUrl": "%{AAIBaseUrl}%",
			"HierarchyDatasetTopLevelName": "dataset",
			"HierarchyDatasetGroupLevelName": "group",
			"HierarchyDatasetDirectLevelName": "direct",
			"SubGroupGrantNames": [ "grp-manage", "grp-edit", "grp-delete", "grp-browse" ],
			"SubDirectGrantNames": [ "ds-manage", "ds-edit", "ds-delete", "ds-browse", "ds-download", "ds-search" ],
			"AutoAssignGrantsOnNewCollection": [ "grp-manage", "grp-edit", "grp-delete", "grp-browse" ],
			"GroupsEndpoint": "groups",
			"GroupEndpoint": "groups/{groupId}",
			"GroupChildrenEndpoint": "groups/{groupId}/children",
			"UserGroupsEndpoint": "users/{userId}/groups",
			"UserGroupsChangeEndpoint": "users/{userId}/groups/{groupId}"
		},
		"Cache": {
			"LookupCache": {
				"Prefix": "%{CacheDisambiguation}%",
				"KeyPattern": "{prefix}:AAI:{kind}:{key}:v0",
				//2 minutes in seconds = 120
				"SlidingExpirationSeconds": 120,
				//5 minutes in seconds = 300
				"AbsoluteExpirationSeconds": 300
			}
		}
	}
}
